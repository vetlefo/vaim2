    "cors": "^2.8.5",
    "cytoscape": "^3.28.1",
    "express": "^4.21.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.1.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.7.4"
  },
  "devDependencies": {
    "@types/cytoscape": "^3.19.16",
    "@types/node": "^20.11.16",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
